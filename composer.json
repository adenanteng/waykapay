{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "guzzlehttp/guzzle": "^7.7.0",
        "inertiajs/inertia-laravel": "^0.6.9",
        "laravel/framework": "^10.17.1",
        "laravel/jetstream": "^2.16.2",
        "laravel/sanctum": "^3.2.5",
        "laravel/tinker": "^2.8.1",
        "protonemedia/inertiajs-tables-laravel-query-builder": "^2.2.0",
        "spatie/laravel-medialibrary": "^10.11.3",
        "spatie/laravel-query-builder": "^5.2.0",
        "stephenjude/laravel-wallet": "^1.0.0",
        "tightenco/ziggy": "^1.6.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.0",
        "laravel/pint": "^1.10.5",
        "laravel/sail": "^1.23.1",
        "mockery/mockery": "^1.6.5",
        "nunomaduro/collision": "^7.8.1",
        "phpunit/phpunit": "^9.6.10",
        "spatie/laravel-ignition": "^2.2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "files": [
        "app/Helpers/Helper.php"
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
